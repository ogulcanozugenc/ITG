{
   "antivirusEngine":{
      "behaviorMonitoring":"enabled",
      "enforcementLevel":"real_time",
      "scanAfterDefinitionUpdate":true,
      "scanArchives":true,
      "enableRealTimeProtection":true,
      "passiveMode":false,
      "maximumOnDemandScanThreads":2,
      "exclusionsMergePolicy":"merge",
      "exclusions":[
         {
            "$type":"excludedPath",
            "isDirectory":false,
            "path":"/var/log/system.log"
         },
         {
            "$type":"excludedPath",
            "isDirectory":true,
            "path":"/etc/zabbix"
         },
		 {
            "$type":"excludedPath",
            "isDirectory":true,
            "path":"/etc/my.cnf.d"
         },
		 {
            "$type":"excludedPath",
            "isDirectory":true,
            "path":"/var/lib/mysql"
         },
         {
            "$type":"excludedPath",
            "isDirectory":true,
            "path":"/home"
         },
         {
            "$type":"excludedFileExtension",
            "extension":"pdf"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"ibd"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"mysql"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"sql"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"frm"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"log"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"cong"
         },
		 {
            "$type":"excludedFileExtension",
            "extension":"cnf"
         },
         {
            "$type":"excludedFileName",
            "name":"cat"
         }
      ],
      "allowedThreats":[
         "EICAR-Test-File (not a virus)"
      ],
      "disallowedThreatActions":[
         "allow",
         "restore"
      ],
      "threatTypeSettingsMergePolicy":"merge",
      "threatTypeSettings":[
         {
            "key":"potentially_unwanted_application",
            "value":"block"
         },
         {
            "key":"archive_bomb",
            "value":"audit"
         }
      ]
   },
   "cloudService":{
      "enabled":true,
      "diagnosticLevel":"optional",
      "automaticSampleSubmissionConsent":"safe",
      "automaticDefinitionUpdateEnabled":true
   }
}